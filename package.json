{
  "name": "refined-prun",
  "version": "1",
  "private": true,
  "description": "Browser extension that simplifies the Prosperous Universe interface and adds useful features",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/refined-prun/refined-prun.git"
  },
  "scripts": {
    "analyze": "vite-bundle-visualizer --sourcemap",
    "build": "pnpm run clean && tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "clean": "rimraf dist",
    "dev": "cross-env __DEV__=true vite build -w --mode development",
    "dev:firefox": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "fix": "pnpx eslint ./ --fix",
    "lint": "pnpx eslint",
    "prepare": "husky",
    "prettier": "prettier . --write --ignore-path .prettierignore"
  },
  "type": "module",
  "dependencies": {
    "chart.js": "^4.4.3",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "dayjs": "^1.11.13",
    "engine.io-parser": "^5.2.3",
    "math-expression-evaluator": "^2.0.5",
    "one-mutation": "^3.0.1",
    "onetime": "^7.0.0",
    "socket.io-parser": "^4.2.4",
    "tinycolor2": "^1.6.0",
    "ts-extras": "^0.13.0",
    "type-fest": "^4.21.0",
    "typed-query-selector": "^2.11.2",
    "uuid": "^10.0.0",
    "vue": "^3.4.37",
    "vue-chartjs": "^5.3.1",
    "vue-draggable-plus": "^0.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@laynezh/vite-plugin-lib-assets": "^0.5.25",
    "@types/chrome": "^0.0.268",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.10",
    "@types/tinycolor2": "^1.4.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "tslib": "^2.6.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^8.0.0",
    "unimport": "^3.13.1",
    "vite": "^5.2.14",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@9.4.0",
  "engines": {
    "node": ">=18.12.0"
  }
}
